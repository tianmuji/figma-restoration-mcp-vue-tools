{
  "mcpServers": {
    "figma-restoration-mcp-vue-tools": {
      "command": "npx",
      "args": ["figma-restoration-mcp-vue-tools", "start"],
      "env": {
        "NODE_ENV": "production"
      }
    },
    "figma": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-figma"],
      "env": {
        "FIGMA_PERSONAL_ACCESS_TOKEN": "{{FIGMA_TOKEN}}"
      }
    }
  }
}
